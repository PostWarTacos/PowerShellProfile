{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
 
	  {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#BBC4B9",
            "properties": {
              "time_format": "15:04:05"
            },
            "style": "plain",
            "template": "[{{ .CurrentDate | date .Format }}] ",
            "type": "time"
          },
          {
            "foreground": "#B6D6F2",
            "leading_diamond": "<#BFA8BF>~</>",
            "properties": {
              "folder_icon": "..",
              "style": "agnoster_short",
              "max_depth": 3,
              "folder_separator_icon": "\\",
              "mapped_locations_enabled": false
            },
            "style": "diamond",
            "template": " \ue5ff {{ .Path }}",
            "trailing_diamond": "",
            "type": "path"
          },
          {
            "foreground": "#BFA8BF",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#fff{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": " \u276f ",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "console_title_template": "{{ .Folder }}",
    "version": 3
  }
